#!/bin/bash -x

source ${STI_SCRIPTS_PATH}/assemble

#curl https://download.newrelic.com/php_agent/release/newrelic-php5-6.4.0.163-linux.tar.gz | tar xz
curl http://localhost/newrelic-php5-6.4.0.163-linux.tar.gz | tar xz

cd newrelic-php5-6.4.0.163-linux
NR_INSTALL_SILENT=1 ./newrelic-install install
